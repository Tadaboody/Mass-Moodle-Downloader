{
    "manifest_version":2,
    "name":"Moodle Downloader Extention",
    "description":"This Extention will download all of a moodle courses files",
    "version":"0.1",
    "browser_action":{
        "default_icon": "icon.png",
        "default_popup":"popup.html"
    },
    "content_scripts":
    [
        {
        "matches": ["https://mw5.haifa.ac.il/course/*"],
        "js":["content.js"],
        "run_ar":"document_end"
        }
    ],
    "permissions": [
        "activeTab",
        "downloads"
    ]
}